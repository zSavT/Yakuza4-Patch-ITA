msgctxt "424"
msgid "Welcome to the blackjack table."
msgstr "Welcome to the blackjack table."

msgctxt "436"
msgid "Let's begin."
msgstr "Let's begin."

msgctxt "448"
msgid ""
"Welcome to the blackjack table. Would you like to play one-on-one against "
"the house?"
msgstr ""
"Welcome to the blackjack table. Would you like to play one-on-one against "
"the house?"

msgctxt "460"
msgid "Let's begin."
msgstr "Let's begin."

msgctxt "472"
msgid "Select \"Bet\" or \"Re-Bet\" to place a wager."
msgstr "Select \"Bet\" or \"Re-Bet\" to place a wager."

msgctxt "484"
msgid "The cards will now be dealt."
msgstr "The cards will now be dealt."

msgctxt "496"
msgid "The dealer pulled a green card. The deck will now be re-shuffled."
msgstr "The dealer pulled a green card. The deck will now be re-shuffled."

msgctxt "508"
msgid "You seem to be short on chips. Please come back soon."
msgstr "You seem to be short on chips. Please come back soon."

msgctxt "520"
msgid "It's your turn. What would you like to do?"
msgstr "It's your turn. What would you like to do?"

msgctxt "532"
msgid "It's a bust. What a shame."
msgstr "It's a bust. What a shame."

msgctxt "544"
msgid "It's the dealer's turn."
msgstr "It's the dealer's turn."

msgctxt "556"
msgid "The total is less than 17. Drawing another card."
msgstr "The total is less than 17. Drawing another card."

msgctxt "568"
msgid "The dealer's total is 17."
msgstr "The dealer's total is 17."

msgctxt "580"
msgid "The dealer's total is 18."
msgstr "The dealer's total is 18."

msgctxt "592"
msgid "The dealer's total is 19."
msgstr "The dealer's total is 19."

msgctxt "604"
msgid "The dealer's total is 20."
msgstr "The dealer's total is 20."

msgctxt "616"
msgid "The dealer's total is 21."
msgstr "The dealer's total is 21."

msgctxt "628"
msgid "The dealer has blackjack."
msgstr "The dealer has blackjack."

msgctxt "640"
msgid "The dealer busts."
msgstr "The dealer busts."

msgctxt "652"
msgid "The dealer's open card is an ace. They could have blackjack."
msgstr "The dealer's open card is an ace. They could have blackjack."

msgctxt "664"
msgid "The dealer has blackjack."
msgstr "The dealer has blackjack."

msgctxt "676"
msgid "The dealer does not have blackjack. The game continues."
msgstr "The dealer does not have blackjack. The game continues."

msgctxt "688"
msgid "You have a blackjack! Congratulations!"
msgstr "You have a blackjack! Congratulations!"

msgctxt "700"
msgid ""
"Unfortunately, the dealer has blackjack as well. Better luck next time."
msgstr ""
"Unfortunately, the dealer has blackjack as well. Better luck next time."

msgctxt "712"
msgid "We look forward to seeing you again soon."
msgstr "We look forward to seeing you again soon."

msgctxt "724"
msgid "You seem to be short on chips. Please visit us again soon."
msgstr "You seem to be short on chips. Please visit us again soon."
