msgctxt "2352"
msgid "Tanimura"
msgstr "Tanimura"

msgctxt "2356"
msgid "Man"
msgstr "Uomo"

msgctxt "2360"
msgid "Kiryu"
msgstr "Kiryu"

msgctxt "2364"
msgid "Akiyama"
msgstr "Akiyama"

msgctxt "2368"
msgid "Saejima"
msgstr "Saejima"

msgctxt "2372"
msgid "Foreign Man"
msgstr "Straniero"

msgctxt "128_Tanimura"
msgid "Excuse me, I'd like to pass through here."
msgstr "Scusi, vorrei passare di qui."

msgctxt "140_Man"
msgid "這里是進入禁止。"
msgstr "Ingresso vietato qui."

msgctxt "152_Tanimura"
msgid "(Doesn't look like he understands Japanese.)"
msgstr "(Non sembra che capisca il giapponese.)"

msgctxt "164_Tanimura"
msgid "\"Excuse me, are you Chinese? I'm a policeman. Could you let me through?\""
msgstr "\"Scusi, è cinese? Sono un poliziotto. Potrebbe farmi passare?\""

msgctxt "176_Man"
msgid "\"Go right ahead.\""
msgstr "\"Prego, vada pure.\""

msgctxt "236_Kiryu"
msgid "Hey, could you move?"
msgstr "Ehi, potresti spostarti?"

msgctxt "248_Akiyama"
msgid "Hey there, pal. Think you could move out of my way?"
msgstr "Ehi, amico. Pensi di poterti togliere dalla mia strada?"

msgctxt "260_Saejima"
msgid "Hey, you're in my way."
msgstr "Ehi, mi stai intralciando."

msgctxt "272_Tanimura"
msgid "This place definitely wouldn't have been chosen."
msgstr "Questo posto non sarebbe stato scelto di certo."

msgctxt "284_Foreign Man"
msgid "這里是進入禁止。"
msgstr "Ingresso vietato qui."

msgctxt "308_Kiryu"
msgid ""
"(A foreigner... Doesn't look like he understands Japanese. I guess I'll "
"avoid Little Asia for now.)"
msgstr ""
"(Uno straniero... Non sembra capire il giapponese. Immagino che eviterò "
"Little Asia per ora.)"

msgctxt "320_Akiyama"
msgid "(Doesn't look like he has any interest in letting me through.)"
msgstr "(Non sembra interessato a farmi passare.)"

msgctxt "332_Saejima"
msgid ""
"(Doesn't look like he's gonna let me through. Best to avoid causin' "
"trouble.)"
msgstr ""
"(Non sembra che mi voglia far passare. Meglio evitare di causare problemi.)"

msgctxt "344_Tanimura"
msgid "This place definitely wouldn't have been chosen."
msgstr "Questo posto non sarebbe stato scelto di certo."
