msgctxt "2384"
msgid "Tanimura"
msgstr "Tanimura"

msgctxt "2388"
msgid "Man"
msgstr "Uomo"

msgctxt "2392"
msgid "Kiryu"
msgstr "Kiryu"

msgctxt "2396"
msgid "Akiyama"
msgstr "Akiyama"

msgctxt "2400"
msgid "Saejima"
msgstr "Saejima"

msgctxt "156_Tanimura"
msgid "Excuse me, I'd like to pass through here."
msgstr "Scusi, vorrei passare di qui."

msgctxt "168_Man"
msgid "這里是進入禁止。"
msgstr "Ingresso vietato qui."

msgctxt "180_Tanimura"
msgid "(Doesn't look like he understands Japanese.)"
msgstr "(Non sembra che capisca il giapponese.)"

msgctxt "192_Tanimura"
msgid "\"Excuse me, are you Chinese? I'm a policeman. Could you let me through?\""
msgstr "\"Scusi, è cinese? Sono un poliziotto. Potrebbe farmi passare?\""

msgctxt "204_Man"
msgid "\"Go right ahead.\""
msgstr "\"Prego, vada pure.\""

msgctxt "264_Kiryu"
msgid "Hey, could you move?"
msgstr "Ehi, potresti spostarti?"

msgctxt "276_Akiyama"
msgid "Hey there. Think you could move out of my way?"
msgstr "Ehi tu. Non è che potresti spostarti dalla mia strada?"

msgctxt "288_Saejima"
msgid "Hey, you're in my way."
msgstr "Ehi, mi stai intralciando."

msgctxt "300_Tanimura"
msgid "This place definitely wouldn't have been chosen."
msgstr "Questo posto non sarebbe stato scelto di certo."

msgctxt "312_Man"
msgid "這里是進入禁止。"
msgstr "Ingresso vietato qui."

msgctxt "336_Kiryu"
msgid ""
"(A foreigner... Doesn't look like he understands Japanese, or wants to let "
"me through.)"
msgstr ""
"(Uno straniero... Non mi sembra capisca il giapponese, o non mi vuole far "
"passare.)"

msgctxt "348_Akiyama"
msgid "(Doesn't look like he speaks Japanese, or wants to let me through.)"
msgstr "Non mi pare capisca il giapponese, o che voglia farmi passare."

msgctxt "360_Saejima"
msgid ""
"(Doesn't look like he's gonna let me through. Best to avoid causin' "
"trouble.)"
msgstr ""
"(Non sembra che mi voglia far passare. Meglio evitare di causare problemi.)"

msgctxt "372_Tanimura"
msgid "This place definitely wouldn't have been chosen."
msgstr "Questo posto non sarebbe stato scelto di certo."
