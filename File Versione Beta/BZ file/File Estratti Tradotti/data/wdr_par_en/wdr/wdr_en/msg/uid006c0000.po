msgctxt "2128"
msgid "Player"
msgstr "Giocatore"

msgctxt "264_Player"
msgid "(Should I give her a present?)"
msgstr "Dovrei farle un regalo?"

msgctxt "288_Player"
msgid " (Maybe she'd like this.)"
msgstr "(Magari le piacerebbe questo.)"

msgctxt "300_Player"
msgid "(On second thought, maybe not.)"
msgstr "(Ripensandoci, magari no.)"

msgctxt "312_Player"
msgid ""
"(This might be a good time to leave, but I've got enough money. Should I "
"stay a bit longer?)"
msgstr ""
"(Forse è il momento di andarsene, ma ho ancora soldi. Mi fermo ancora un "
"po'?)"

msgctxt "324_Player"
msgid " (I think I'll stay just a bit longer.)"
msgstr "(Penso che mi fermerò ancora un po'.)"

msgctxt "336_Player"
msgid "(I think it's time to go.)"
msgstr "(Penso sia ora di andare.)"

msgctxt "348_Player"
msgid "(What should I talk about?)"
msgstr "(Di cosa dovrei parlare?)"

msgctxt "396_Player"
msgid "Yes"
msgstr "Sì"

msgctxt "408_Player"
msgid "No"
msgstr "No"

msgctxt "420_Player"
msgid "Stay"
msgstr "Resta"

msgctxt "432_Player"
msgid "Leave"
msgstr "Andarsene"

msgctxt "444_Player"
msgid "The rumor about ramen."
msgstr "La voce sul ramen."

msgctxt "456_Player"
msgid "The rumor about gelato."
msgstr "La voce sul gelato."

msgctxt "468_Player"
msgid "The rumor about fake brand-name goods."
msgstr "La voce sulla roba firmata tarocca."
