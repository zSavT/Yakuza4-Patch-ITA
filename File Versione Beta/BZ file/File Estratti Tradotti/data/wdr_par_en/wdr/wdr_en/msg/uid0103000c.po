msgctxt "1404"
msgid "Nagaharu"
msgstr "Nagaharu"

msgctxt "232_Nagaharu"
msgid "Let's pick the banker."
msgstr "Scegliamo il banchiere."

msgctxt "244_Nagaharu"
msgid "So I'm the banker. Let's place our bets."
msgstr "Allora, il banco sono io. Piazzate le puntate."

msgctxt "256_Nagaharu"
msgid "How much do you wanna wager?"
msgstr "Quanto vuoi puntare?"

msgctxt "268_Nagaharu"
msgid "Let's do this!"
msgstr "Facciamolo!"

msgctxt "280_Nagaharu"
msgid "My turn!"
msgstr "Tocca a me!"

msgctxt "292_Nagaharu"
msgid "One more time."
msgstr "Ancora una volta."

msgctxt "304_Nagaharu"
msgid "Let's settle our bets."
msgstr "Regoliamo i conti."

msgctxt "316_Nagaharu"
msgid "You're the banker."
msgstr "Sei il banchiere."

msgctxt "328_Nagaharu"
msgid "Let's change bankers."
msgstr "Dai, cambiamo banchiere."

msgctxt "340_Nagaharu"
msgid "Wanna play again?"
msgstr "Ti va di giocare ancora?"

msgctxt "352_Nagaharu"
msgid "I'm spent."
msgstr "Sono al verde."

msgctxt "364_Nagaharu"
msgid "Better luck next time."
msgstr "Sar√† per la prossima volta."
