msgctxt "112"
msgid "Meet with Kido?"
msgstr "Incontrare Kido?"

msgctxt "1356"
msgid "Akiyama"
msgstr "Akiyama"

msgctxt "100_Akiyama"
msgid "(This is where I'm supposed to meet Kido-chan. I can access the roof from here.)"
msgstr "(È qui che dovrei incontrare Kido-chan. Posso accedere al tetto da qui.)"

msgctxt "148_Akiyama"
msgid "(Maybe later.)"
msgstr "(Magari più tardi.)"

msgctxt "184_Akiyama"
msgid "(All right. Guess I'll go meet Kido-chan.)"
msgstr "(Va bene. Credo che andrò a incontrare Kido-chan.)"

msgctxt "208_Akiyama"
msgid "Yes"
msgstr "Sì"

msgctxt "220_Akiyama"
msgid "No"
msgstr "No"