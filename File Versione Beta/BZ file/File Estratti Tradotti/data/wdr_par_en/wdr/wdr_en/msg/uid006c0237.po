msgid "Ehehe... I wonder what you'll get me! <Color:7>♥<Color:Default>"
msgstr "Ehehe... mi chiedo cosa mi prenderai! <Color:7>♥<Color:Default>"

msgctxt "4872_Talk_Yes"
