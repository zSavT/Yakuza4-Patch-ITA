msgid "Tanimura"
msgstr "Tanimura"

msgctxt "4220"
msgid "Man in a Business Suit"
msgstr "Uomo in completo"

msgctxt "4224"
msgid "Talk_Yes"
msgstr "Talk_Yes"

msgctxt "4228"
