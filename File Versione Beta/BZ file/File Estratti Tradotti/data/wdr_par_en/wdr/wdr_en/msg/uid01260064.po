msgctxt "600"
msgid "Welcome to the baccarat table."
msgstr "Welcome to the baccarat table."

msgctxt "612"
msgid "Let's begin."
msgstr "Let's begin."

msgctxt "624"
msgid "We look forward to seeing you again soon."
msgstr "We look forward to seeing you again soon."

msgctxt "636"
msgid "You seem to be short on chips. Please come visit us again soon."
msgstr "You seem to be short on chips. Please come visit us again soon."

msgctxt "648"
msgid ""
"Please select what you will bet on:\r\n"
"the Player, the Banker, or a tie."
msgstr ""
"Please select what you will bet on:\r\n"
"the Player, the Banker, or a tie."

msgctxt "660"
msgid "Select a wager between the minimum and maximum betting limit."
msgstr "Select a wager between the minimum and maximum betting limit."

msgctxt "672"
msgid "I will now deal the cards."
msgstr "I will now deal the cards."

msgctxt "684"
msgid "I'll reveal the Player's first card."
msgstr "I'll reveal the Player's first card."

msgctxt "696"
msgid "I'll reveal the Player's second card."
msgstr "I'll reveal the Player's second card."

msgctxt "708"
msgid "The Player stands with 6."
msgstr "The Player stands with 6."

msgctxt "720"
msgid "The Player stands with 7."
msgstr "The Player stands with 7."

msgctxt "732"
msgid "The Player has a natural 8."
msgstr "The Player has a natural 8."

msgctxt "744"
msgid "The Player has a natural 9."
msgstr "The Player has a natural 9."

msgctxt "756"
msgid ""
"A third card will be dealt if the Banker does\r\n"
"not have an 8 or a 9."
msgstr ""
"A third card will be dealt if the Banker does\r\n"
"not have an 8 or a 9."

msgctxt "768"
msgid "I'll reveal the Banker's first card."
msgstr "I'll reveal the Banker's first card."

msgctxt "780"
msgid "I'll reveal the Banker's second card."
msgstr "I'll reveal the Banker's second card."

msgctxt "792"
msgid "The Banker stands with 6."
msgstr "The Banker stands with 6."

msgctxt "804"
msgid ""
"The Banker stands with 6.\r\n"
"I'll deal a third card to the Player."
msgstr ""
"The Banker stands with 6.\r\n"
"I'll deal a third card to the Player."

msgctxt "816"
msgid "The Banker stands with 7."
msgstr "The Banker stands with 7."

msgctxt "828"
msgid ""
"The Banker stands with 7.\r\n"
"I'll deal a third card to the Player."
msgstr ""
"The Banker stands with 7.\r\n"
"I'll deal a third card to the Player."

msgctxt "840"
msgid "The Banker has a natural 8."
msgstr "The Banker has a natural 8."

msgctxt "852"
msgid "The Banker has a natural 9."
msgstr "The Banker has a natural 9."

msgctxt "864"
msgid ""
"A third card will be dealt to the Banker, depending on the Player's third "
"card."
msgstr ""
"A third card will be dealt to the Banker, depending on the Player's third "
"card."

msgctxt "876"
msgid ""
"The Player stands.\r\n"
"I'll deal a third card to the Banker."
msgstr ""
"The Player stands.\r\n"
"I'll deal a third card to the Banker."

msgctxt "888"
msgid "I'll reveal the Player's third card."
msgstr "I'll reveal the Player's third card."

msgctxt "900"
msgid "Due to the outcome, the Banker will not be dealt a third card."
msgstr "Due to the outcome, the Banker will not be dealt a third card."

msgctxt "912"
msgid "Due to the outcome, the Banker will be dealt a third card."
msgstr "Due to the outcome, the Banker will be dealt a third card."

msgctxt "924"
msgid "I'll reveal the Banker's third card."
msgstr "I'll reveal the Banker's third card."

msgctxt "936"
msgid "The Player wins."
msgstr "The Player wins."

msgctxt "948"
msgid "The Banker wins."
msgstr "The Banker wins."

msgctxt "960"
msgid "It's a tie."
msgstr "It's a tie."

msgctxt "972"
msgid "Congratulations."
msgstr "Congratulations."

msgctxt "984"
msgid "That's too bad."
msgstr "That's too bad."

msgctxt "996"
msgid "Would you like to continue?"
msgstr "Would you like to continue?"

msgctxt "1008"
msgid "Here you are."
msgstr "Here you are."

msgctxt "1020"
msgid "The deck is running low, so I'll shuffle."
msgstr "The deck is running low, so I'll shuffle."
