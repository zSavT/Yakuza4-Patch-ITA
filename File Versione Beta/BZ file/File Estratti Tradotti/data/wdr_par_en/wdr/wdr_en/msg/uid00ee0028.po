msgctxt "1444"
msgid "Manager"
msgstr "Direttore"

msgctxt "1448"
msgid "Talk_Ojigi"
msgstr "Talk_Ojigi"

msgctxt "144_Manager"
msgid "Hello, sir. What can I do for you today?"
msgstr "Salve, signore. Cosa posso fare per lei oggi?"

msgctxt "156_Manager"
msgid "What would you like to purchase?"
msgstr "Cosa desidera acquistare?"

msgctxt "216_Manager"
msgid "Thank you very much."
msgstr "Molte grazie."

msgctxt "228_Manager"
msgid "What do you have to sell?"
msgstr "Cosa ha da vendere?"

msgctxt "252_Manager"
msgid "Thank you for your patronage."
msgstr "Grazie per la sua preferenza."

msgctxt "264_Manager"
msgid "I'm here to buy."
msgstr "Sono qui per comprare."

msgctxt "276_Manager"
msgid "I'm here to sell."
msgstr "Sono qui per vendere."

msgctxt "288_Manager"
msgid "Never mind."
msgstr "Lascia perdere."
