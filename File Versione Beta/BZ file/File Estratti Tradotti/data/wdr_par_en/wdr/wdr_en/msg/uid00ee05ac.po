msgid "You got a <Color:8>Small Bomb<Color:Default>."
msgstr "Hai una <Color:8>Bomba Piccola<Color:Default>."

msgctxt "704"
msgid "You got a <Color:8>Superalloy Pickaxe<Color:Default>."
msgstr "Hai ottenuto un <Color:8>Piccone di Superlega<Color:Default>."

msgctxt "776"
msgid "You got a K Works Catalog 1."
msgstr "Ti sei beccato un K Works Catalogo 1."

msgctxt "920"
msgid "You got a K Works Catalog 3."
msgstr "Hai preso un Catalogo K Works 3."

