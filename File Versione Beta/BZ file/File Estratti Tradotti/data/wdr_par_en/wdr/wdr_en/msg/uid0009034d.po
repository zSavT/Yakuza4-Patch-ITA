msgctxt "152"
msgid "You got the <Color:8>Briefcase with Jewels<Color:Default>."
msgstr "Hai ottenuto la <Color:8>Valigetta con gioielli<Color:Default>."

msgctxt "1984"
msgid "Jewel Thief"
msgstr "Ladro di gioielli"

msgctxt "1988"
msgid "Saejima"
msgstr "Saejima"

