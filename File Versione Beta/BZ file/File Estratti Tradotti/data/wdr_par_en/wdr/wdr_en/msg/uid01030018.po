msgctxt "1444"
msgid "Thick-cut Tetsu"
msgstr "Thick-cut Tetsu"

msgctxt "232_Thick-cut Tetsu"
msgid " Let's pick the banker."
msgstr "Scegliamo il banchiere."

msgctxt "244_Thick-cut Tetsu"
msgid "I'm the banker."
msgstr "Sono il banchiere."

msgctxt "256_Thick-cut Tetsu"
msgid "How much will you bet?"
msgstr "Quanto vuoi puntare?"

msgctxt "268_Thick-cut Tetsu"
msgid "Let's start."
msgstr "Si parte!"

msgctxt "280_Thick-cut Tetsu"
msgid "It's my turn."
msgstr "È il mio turno."

msgctxt "292_Thick-cut Tetsu"
msgid "(This line signifies failure.)"
msgstr "(Questa riga indica fallimento.)"

msgctxt "304_Thick-cut Tetsu"
msgid "Let's settle up."
msgstr "Saldiamo i conti."

msgctxt "316_Thick-cut Tetsu"
msgid "You're the banker."
msgstr "Sei il banchiere."

msgctxt "328_Thick-cut Tetsu"
msgid "Time to change the banker."
msgstr "Si cambia il banco."

msgctxt "340_Thick-cut Tetsu"
msgid "Play again?"
msgstr "Ancora una?"

msgctxt "352_Thick-cut Tetsu"
msgid "I'm tapped out. Looks like it's time to head home."
msgstr "Sono rimasto a secco. Mi sa che è ora di tornare a casa."

msgctxt "364_Thick-cut Tetsu"
msgid "That it for you? Better luck next time."
msgstr "È finita qui per te? Più fortuna la prossima volta."
