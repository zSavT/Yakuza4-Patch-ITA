msgid "Akiyama"
msgstr "Akiyama"

msgctxt "17264"
msgid "Achieve Super Service 5 Times"
msgstr "Ottieni il Super Servizio 5 volte"

msgctxt "17268"
msgid "Mastering Cosplay Day"
msgstr "Padroneggiare il Giorno del Cosplay"

msgctxt "17272"
msgid "Aim for the Top!"
msgstr "Punta al vertice!"

msgctxt "17276"
msgid "Talk_Smile"
msgstr "Talk_Smile"

msgctxt "17280"
