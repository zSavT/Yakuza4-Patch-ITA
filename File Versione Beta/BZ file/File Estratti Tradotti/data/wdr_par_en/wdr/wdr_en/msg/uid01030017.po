msgctxt "1536"
msgid "Stormy Kaneshiro"
msgstr "Stormy Kaneshiro"

msgctxt "232_Stormy Kaneshiro"
msgid "Let's pick the banker."
msgstr "Scegliamo il banchiere."

msgctxt "244_Stormy Kaneshiro"
msgid "Looks like I'm the banker."
msgstr "Mi sa che sono io il banchiere."

msgctxt "256_Stormy Kaneshiro"
msgid "Place yer bets!"
msgstr "Piazzate le scommesse!"

msgctxt "268_Stormy Kaneshiro"
msgid "Get ready, here she goes!"
msgstr "Attenti, eccola che parte!"

msgctxt "280_Stormy Kaneshiro"
msgid "My turn."
msgstr "Tocca a me."

msgctxt "292_Stormy Kaneshiro"
msgid "(This line signifies failure.)"
msgstr "(Questa riga indica fallimento.)"

msgctxt "304_Stormy Kaneshiro"
msgid "Time to settle up."
msgstr "È ora di saldare i conti."

msgctxt "316_Stormy Kaneshiro"
msgid "You're the banker, pal."
msgstr "Sei tu il banco, compare."

msgctxt "328_Stormy Kaneshiro"
msgid "Time to change it up."
msgstr "È ora di dare una svolta."

msgctxt "340_Stormy Kaneshiro"
msgid "Go one more?"
msgstr "Un altro giro?"

msgctxt "352_Stormy Kaneshiro"
msgid "Aw brother, that's all I can take. Be seein' ya."
msgstr "Ah, compa', non ne posso più. Ci si becca."

msgctxt "364_Stormy Kaneshiro"
msgid "Heh! Yer all cleaned out! Better save up for next time."
msgstr "Heh! Ti hanno ripulito! Metti via qualche soldo per il prossimo giro."
