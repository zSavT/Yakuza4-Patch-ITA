msgctxt "1320"
msgid "Akiyama"
msgstr "Akiyama"

msgctxt "232_Akiyama"
msgid "Let's choose the banker."
msgstr "Scegliamo il banchiere."

msgctxt "244_Akiyama"
msgid "Guess I'm the banker."
msgstr "Mi sa che sono io il banchiere."

msgctxt "256_Akiyama"
msgid "(How much should I wager?)"
msgstr "(Quanto dovrei puntare?)"

msgctxt "268_Akiyama"
msgid "Let's start the match."
msgstr "Si dà il via alla partita."

msgctxt "280_Akiyama"
msgid "Looks like it's my turn."
msgstr "Pare sia il mio turno."

msgctxt "292_Akiyama"
msgid "I'll roll again."
msgstr "Tiro di nuovo."

msgctxt "304_Akiyama"
msgid "Let's settle up."
msgstr "Saldiamo i conti."

msgctxt "316_Akiyama"
msgid "(You're the banker.)"
msgstr "(Sei il banchiere.)"

msgctxt "328_Akiyama"
msgid "Why don't we change the banker."
msgstr "Perché non cambiamo il banchiere?"

msgctxt "340_Akiyama"
msgid "(Play again?)"
msgstr "(Facciamo un altro giro?)"

msgctxt "352_Akiyama"
msgid "(I'm broke.)"
msgstr "(Sono al verde.)"

msgctxt "364_Akiyama"
msgid "(Try again next time.)"
msgstr "(Riprova la prossima volta.)"
