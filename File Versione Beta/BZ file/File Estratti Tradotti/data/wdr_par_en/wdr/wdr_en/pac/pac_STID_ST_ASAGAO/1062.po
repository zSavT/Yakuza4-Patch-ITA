msgctxt "160"
msgid "<Color:7>Komaki Tiger Drop<Color:Default>!"
msgstr "<Color:7>Controcolpo della Tigre di Komaki<Color:Default>!"

msgctxt "172"
msgid ""
"That's it! Now, use a\r\n"
"<Color:7>Komaki Tiger Drop<Color:Default>!"
msgstr "Proprio così! Ora, usa un\r\n<Color:7>Controcolpo della Tigre di Komaki<Color:Default>!"

msgctxt "184"
msgid "Go! <Color:7>Komaki Tiger Drop<Color:Default>!"
msgstr "Vai! <Color:7>Controcolpo della Tigre di Komaki<Color:Default>!"

msgctxt "196"
msgid ""
"Don't give up,\r\n"
"Uncle Kaz!"
msgstr "Non arrenderti,\r\nZio Kaz!"

msgctxt "208"
msgid "<Color:7>Double Finisher<Color:Default>!"
msgstr "<Color:7>Doppio Colpo di Grazia<Color:Default>!"

msgctxt "220"
msgid ""
"Next... do a\r\n"
"<Color:7>Double Finisher<Color:Default>!"
msgstr "Ora... fai un\r\n<Color:7>Doppio Colpo di Grazia<Color:Default>!"

msgctxt "232"
msgid "Now do another <Color:7>Double Finisher<Color:Default>!"
msgstr "Ora fai un altro <Color:7>Doppio Colpo di Grazia<Color:Default>!"

msgctxt "244"
msgid ""
"Be careful,\r\n"
"Uncle Kaz!"
msgstr "Fai attenzione,\r\nZio Kaz!"

msgctxt "256"
msgid ""
"Go, go!\r\n"
"<Color:7>Dropkick<Color:Default>!"
msgstr "Vai, vai!\r\n<Color:7>Dropkick<Color:Default>!"

msgctxt "268"
msgid "Do a <Color:7>Dropkick<Color:Default>!"
msgstr "Fai un <Color:7>Dropkick<Color:Default>!"

msgctxt "280"
msgid ""
"Do a <Color:7>Dropkick<Color:Default>!\r\n"
"<Color:7>Dropkick<Color:Default>!"
msgstr "Fai un <Color:7>Dropkick<Color:Default>!\r\n<Color:7>Dropkick<Color:Default>!"

msgctxt "292"
msgid ""
"Don't lose,\r\n"
"Uncle Kaz!"
msgstr "Non perdere,\r\nZio Kaz!"

msgctxt "304"
msgid "Show me a <Color:7>Komaki Cat-Like Reflexes<Color:Default>!"
msgstr "Fammi vedere i <Color:7>Riflessi Felini di Komaki<Color:Default>!"

msgctxt "316"
msgid ""
"Watch out!\r\n"
"<Color:7>Komaki Cat-Like Reflexes<Color:Default>!"
msgstr "Attento!\r\n<Color:7>Riflessi Felini di Komaki<Color:Default>!"

msgctxt "328"
msgid "<Color:7>Komaki Cat-Like Reflexes<Color:Default>!"
msgstr "<Color:7>Riflessi Felini di Komaki<Color:Default>!"

msgctxt "340"
msgid ""
"You can do it,\r\n"
"Uncle Kaz!"
msgstr "Puoi farcela,\r\nZio Kaz!"

msgctxt "352"
msgid "Get 'em with a <Color:7>Komaki Dodge Shot<Color:Default>!"
msgstr "Prendili con un <Color:7>Colpo in Schivata di Komaki<Color:Default>!"

msgctxt "364"
msgid ""
"Go Uncle Kaz! Use a\r\n"
"<Color:7>Komaki Dodge Shot<Color:Default>!"
msgstr "Vai Zio Kaz! Usa un\r\n<Color:7>Colpo in Schivata di Komaki<Color:Default>!"

msgctxt "376"
msgid ""
"<Color:7>Komaki Dodge Shot<Color:Default>\r\n"
"C'mon!"
msgstr "<Color:7>Colpo in Schivata di Komaki<Color:Default>\r\nDai!"

msgctxt "388"
msgid ""
"Don't lose,\r\n"
"Uncle Kaz!"
msgstr "Non perdere,\r\nZio Kaz!"

msgctxt "400"
msgid ""
"Now use...\r\n"
"<Color:7>Knockdown Reversal<Color:Default>!"
msgstr "Ora usa...\r\n<Color:7>Inversione a Terra<Color:Default>!"

msgctxt "412"
msgid "<Color:7>Knockdown Reversal<Color:Default>!"
msgstr "<Color:7>Inversione a Terra<Color:Default>!"

msgctxt "424"
msgid ""
"<Color:7>Knockdown Reversal<Color:Default>\r\n"
"C'mon!"
msgstr "<Color:7>Inversione a Terra<Color:Default>\r\nDai!"

msgctxt "436"
msgid ""
"Now's your chance,\r\n"
"Uncle Kaz!"
msgstr "Ora è la tua occasione,\r\nZio Kaz!"

msgctxt "448"
msgid ""
"Finish them off with a\r\n"
"<Color:7>True Lotus Flare Fist<Color:Default>!"
msgstr "Finiscili con un\r\n<Color:7>Vero Pugno di Fuoco del Loto<Color:Default>!"

msgctxt "460"
msgid ""
"Do a <Color:7>True Lotus Flare Fist<Color:Default>\r\n"
"to finish 'em off!"
msgstr "Fai un <Color:7>Vero Pugno di Fuoco del Loto<Color:Default>\r\nper finirli!"

msgctxt "472"
msgid ""
"<Color:7>True Lotus Flare Fist<Color:Default>!\r\n"
"C'mon, you can do it!"
msgstr "<Color:7>Vero Pugno di Fuoco del Loto<Color:Default>!\r\nDai, puoi farcela!"

msgctxt "484"
msgid "Uncle Kaz!"
msgstr "Zio Kaz!"