msgctxt "160"
msgid ""
"Uncle Kaz!\r\n"
"<Color:7>Komaki Tiger Drop<Color:Default>!"
msgstr "Zio Kaz!\r\n<Color:7>Controcolpo della Tigre di Komaki<Color:Default>!"

msgctxt "172"
msgid ""
"Now!\r\n"
"<Color:7>Komaki Tiger Drop<Color:Default>!"
msgstr "Ora!\r\n<Color:7>Controcolpo della Tigre di Komaki<Color:Default>!"

msgctxt "184"
msgid "Now use a <Color:7>Komaki Tiger Drop<Color:Default>!"
msgstr "Ora usa un <Color:7>Controcolpo della Tigre di Komaki<Color:Default>!"

msgctxt "196"
msgid ""
"You can do it,\r\n"
"Uncle Kaz!"
msgstr "Puoi farcela,\r\nZio Kaz!"

msgctxt "208"
msgid ""
"Uncle Kaz! <Color:7>Double Finisher<Color:Default>!\r\n"
"Come on, do a <Color:7>Double Finisher<Color:Default>!"
msgstr "Zio Kaz! <Color:7>Doppio Colpo di Grazia<Color:Default>!\r\nDai, fai un <Color:7>Doppio Colpo di Grazia<Color:Default>!"

msgctxt "220"
msgid ""
"Now! Do a\r\n"
"<Color:7>Double Finisher<Color:Default>!"
msgstr "Ora! Fai un\r\n<Color:7>Doppio Colpo di Grazia<Color:Default>!"

msgctxt "232"
msgid "Now use a <Color:7>Double Finisher<Color:Default>!"
msgstr "Ora usa un <Color:7>Doppio Colpo di Grazia<Color:Default>!"

msgctxt "244"
msgid ""
"Watch out,\r\n"
"Uncle Kaz!"
msgstr "Attento,\r\nZio Kaz!"

msgctxt "256"
msgid ""
"Keep it up!\r\n"
"<Color:7>Dropkick<Color:Default>!"
msgstr "Continua così!\r\n<Color:7>Dropkick<Color:Default>!"

msgctxt "268"
msgid "<Color:7>Dropkick<Color:Default>!"
msgstr "<Color:7>Dropkick<Color:Default>!"

msgctxt "280"
msgid ""
"Use a <Color:7>Dropkick<Color:Default>!\r\n"
"<Color:7>Dropkick<Color:Default>, c'mon!"
msgstr "Usa un <Color:7>Dropkick<Color:Default>!\r\n<Color:7>Dropkick<Color:Default>, dai!"

msgctxt "292"
msgid ""
"That's it,\r\n"
"Uncle Kaz!"
msgstr "Proprio così,\r\nZio Kaz!"

msgctxt "304"
msgid "<Color:7>Komaki Cat-Like Reflexes<Color:Default>!"
msgstr "<Color:7>Riflessi Felini di Komaki<Color:Default>!"

