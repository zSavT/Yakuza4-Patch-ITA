msgctxt "329\t408"
msgid "Where the fuck are ya, bro?"
msgstr "Dove cazzo sei, fratello?"

msgctxt "407\t450"
msgid "Late-ass..."
msgstr "Perditempo..."

msgctxt "970\t1081"
msgid "It's me.\\nSaejima, Sasai Family."
msgstr "Sono io.\\nSaejima, Famiglia Sasai."

msgctxt "1092\t1190"
msgid "Majima was s'posed to be here by now."
msgstr "Majima doveva già essere qui."

msgctxt "1191\t1249"
msgid "What's holdin' him up?"
msgstr "Cosa lo trattiene?"

msgctxt "1262\t1336"
msgid "Yup. I'll be－"
msgstr "Sì. Sarò-"

msgctxt "1451\t1497"
msgid "They're here."
msgstr "Sono qui."

msgctxt "1525\t1630"
msgid "Never mind, I'm good.\\nLater."
msgstr "Non importa, va bene.\\nA dopo."

msgctxt "1768\t1821"
msgid "Goddamn."
msgstr "Cavolo."

msgctxt "1838\t1920"
msgid "That ain't 10.\\nThat's a fuckin' army."
msgstr "Non sono 10.\\nÈ un'armata."

msgctxt "5094\t5138"
msgid "You're gonna pay for this!"
msgstr "La pagherai per questo!"

msgctxt "6053\t6131"
msgid "No! Stay back!"
msgstr "No! Indietro!"

msgctxt "7760\t7820"
msgid "Please..."
msgstr "Ti prego..."

msgctxt "7822\t7867"
msgid "Not the boss."
msgstr "Non il capo."

msgctxt "7868\t7985"
msgid "I swore I'd protect him with my life!"
msgstr "Giuro di proteggerlo con la vita!"

msgctxt "8339\t8399"
msgid "Sorry..."
msgstr "Scusa..."

msgctxt "8414\t8498"
msgid "But I got my orders too."
msgstr "Ma anche io ho i miei ordini."

msgctxt "8515\t8575"
msgid "Ueno-san."
msgstr "Ueno-san."

msgctxt "8588\t8648"
msgid "It's nothin' personal."
msgstr "Nulla di personale."

msgctxt "8662\t8721"
msgid "Someone help...!"
msgstr "Qualcuno mi aiuti...!"

