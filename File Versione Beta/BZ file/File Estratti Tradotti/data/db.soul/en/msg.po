msgctxt "1\t26011\t181\tCHK"
msgid "Keep it up..."
msgstr "Continua così..."

msgctxt "1\t26025\t182\tCHK"
msgid "Guess that works."
msgstr "Immagino che funzioni."

msgctxt "1\t26043\t183\tCHK"
msgid "Don't you make fun of me."
msgstr "Non prendermi in giro."

msgctxt "1\t26069\t184\tCHK"
