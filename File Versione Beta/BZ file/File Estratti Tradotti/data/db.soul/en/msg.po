msgctxt "1\t25514\t159\tCHK"
msgid "Heh, that was easy."
msgstr "Eh, è stato facile."

msgctxt "1\t25534\t160\tCHK"
msgid "Now's not the time to be sleeping!"
msgstr "Non è il momento di dormire!"

msgctxt "1\t25569\t161\tCHK"
msgid "I don't feel like losing."
msgstr "Non mi va di perdere."

msgctxt "1\t25595\t162\tCHK"
msgid "Too early to celebrate!"
msgstr "È troppo presto per festeggiare!"

msgctxt "1\t25619\t163\tCHK"
msgid "Crap!"
msgstr "Cavolo!"

msgctxt "1\t25625\t164\tCHK"
