msgctxt "1\t23624\t73\tCHK"
msgid "I can't lose to you."
msgstr "Non posso perdere contro di te."

msgctxt "1\t23645\t74\tCHK"
msgid "What to have for dinner...?"
msgstr "Cosa mangio per cena...?"

msgctxt "1\t23673\t75\tCHK"
