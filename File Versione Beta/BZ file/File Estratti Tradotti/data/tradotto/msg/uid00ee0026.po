msgctxt "2352"
msgid "Tanimura"
msgstr "Tanimura"

msgctxt "2356"
msgid "Man"
msgstr "Uomo"

msgctxt "2360"
msgid "Kiryu"
msgstr "Kiryu"

msgctxt "2364"
msgid "Akiyama"
msgstr "Akiyama"

msgctxt "2368"
msgid "Saejima"
msgstr "Saejima"

msgctxt "2372"
msgid "Foreign Man"
msgstr "Straniero"

msgctxt "128_Tanimura"
msgid "Excuse me, I'd like to pass through here."
msgstr "Permesso, vorrei passare di qui."

msgctxt "140_Man"
msgid "這里是進入禁止。"
msgstr "Ingresso vietato qui."

msgctxt "152_Tanimura"
msgid "(Doesn't look like he understands Japanese.)"
msgstr "(Non sembra proprio che capisca il giapponese.)"

msgctxt "164_Tanimura"
msgid "\"Excuse me, are you Chinese? I'm a policeman. Could you let me through?\""
msgstr "Scusi, è cinese? Sono un poliziotto. Potrebbe farmi passare?"

msgctxt "176_Man"
msgid "\"Go right ahead.\""
msgstr "Vai pure."

msgctxt "236_Kiryu"
msgid "Hey, could you move?"
msgstr "Ehi, potresti spostarti?"

msgctxt "248_Akiyama"
msgid "Hey there, pal. Think you could move out of my way?"
msgstr "Senti, amico. Potresti farti da parte?"

msgctxt "260_Saejima"
msgid "Hey, you're in my way."
msgstr "Ehi, sei in mezzo ai piedi."

msgctxt "272_Tanimura"
msgid "This place definitely wouldn't have been chosen."
msgstr "Questo posto, di certo, non l'avrebbero scelto."

msgctxt "284_Foreign Man"
msgid "這里是進入禁止。"
msgstr "Ingresso vietato qui."

msgctxt "308_Kiryu"
msgid ""
"(A foreigner... Doesn't look like he understands Japanese. I guess I'll "
"avoid Little Asia for now.)"
msgstr ""
"(Uno straniero... Non sembra capire il giapponese. Credo che per ora eviterò"
" Little Asia.)"

msgctxt "320_Akiyama"
msgid "(Doesn't look like he has any interest in letting me through.)"
msgstr "Non mi pare proprio intenzionato a farmi passare."

msgctxt "332_Saejima"
msgid ""
"(Doesn't look like he's gonna let me through. Best to avoid causin' "
"trouble.)"
msgstr "(Non mi farà passare, pare. Meglio evitare di fare casino.)"

msgctxt "344_Tanimura"
msgid "This place definitely wouldn't have been chosen."
msgstr "Questo posto, di certo, non l'avrebbero scelto."
