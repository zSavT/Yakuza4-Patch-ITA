msgctxt "248"
msgid "You got the <Color:8>Briefcase with 30 Million Yen<Color:Default>."
msgstr "Hai la <Color:8>Valigetta con 30 milioni di Yen<Color:Default>."

msgctxt "2880"
msgid "Akiyama"
msgstr "Akiyama"

msgctxt "2884"
msgid "Hana"
msgstr "Hana"

