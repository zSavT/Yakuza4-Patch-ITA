msgctxt "132"
msgid ""
"Your pack is too full with items.\r\n"
"This will activate when you get another item."
msgstr ""
"Il tuo inventario è troppo pieno di oggetti.\n"
"Si attiverà quando otterrai un altro oggetto."

msgctxt "144"
msgid "Throw away: got 1 item"
msgstr "Butta via: 1 oggetto"

msgctxt "156"
msgid "Throw away: got a full stack of items"
msgstr "Scarta: hai uno stack completo di oggetti"

msgctxt "168"
msgid "Send: got 1 item."
msgstr "Inviato: Hai ottenuto 1 oggetto."

msgctxt "180"
msgid "Send: got a full stack of items."
msgstr "Invia: lo stack di oggetti è pieno."

msgctxt "216"
msgid "Throw away: item acquisition → 1"
msgstr "Scarta: acquisizione oggetto → 1"

msgctxt "228"
msgid "Throw away: item acquisition → full"
msgstr "Scarta: acquisizione oggetto → pieno"

msgctxt "240"
msgid "Send: item acquisition → 1"
msgstr "Invia: acquisizione oggetto → 1"

msgctxt "252"
msgid "Send: item acquisition → full"
msgstr "Invio: acquisizione oggetto → pieno"
