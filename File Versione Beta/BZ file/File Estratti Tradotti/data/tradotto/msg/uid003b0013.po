msgctxt "1020"
msgid "マック"
msgstr "Mac"

msgctxt "172_マック"
msgid "There's nothin' like that!"
msgstr "Non c'è niente del genere!"

msgctxt "196_マック"
msgid "Like I'd tell you!"
msgstr "E come no!"

msgctxt "220_マック"
msgid "Tell me my opponent's skill"
msgstr "Dimmi di che pasta è fatto il mio avversario."

msgctxt "232_マック"
msgid "Tell me how to avoid impossible battles"
msgstr "Dimmi come evitare battaglie impossibili"

msgctxt "244_マック"
msgid "Tell me how to lose my pursuers"
msgstr "Dimmi come seminare i miei inseguitori"

msgctxt "256_マック"
msgid "Tell me how to quickly catch my opponent"
msgstr "Dimmi come prendere velocemente il mio avversario"

msgctxt "268_マック"
msgid "Tell me how to find shortcuts in town"
msgstr "Dimmi come trovare scorciatoie in città"
