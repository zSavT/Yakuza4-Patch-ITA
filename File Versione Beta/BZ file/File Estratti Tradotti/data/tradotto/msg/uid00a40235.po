msgid "Man in Black"
msgstr "L'Uomo in Nero"

msgctxt "5004"
msgid "Saejima"
msgstr "Saejima"

msgctxt "5008"
msgid "Talk_No"
msgstr "Talk_No"

msgctxt "5012"
msgid "Talk_Ojigi"
msgstr "Talk_Ojigi"

