msgctxt "2\t55216\t343\tCHK"
msgid "There goes my pay..."
msgstr "Ecco la mia paga..."

msgctxt "2\t55237\t344\tCHK"
msgid "My wife's so annoying."
msgstr "Mia moglie è così fastidiosa."

msgctxt "2\t55260\t345\tCHK"
msgid "Maybe I should quit smoking."
msgstr "Forse dovrei smettere di fumare."

msgctxt "2\t55289\t346\tCHK"
msgid "Aren't you aggressive?"
msgstr "Non sei aggressivo?"

msgctxt "2\t55312\t347\tCHK"
