msgctxt "2\t52819\t239\tCHK"
msgid "These tiles aren't so great..."
msgstr "Queste tessere non sono un granché..."

msgctxt "2\t52850\t240\tCHK"
msgid "Tch... Knew this'd happen."
msgstr "Tch... Sapevo che sarebbe successo."

msgctxt "2\t52877\t241\tCHK"
msgid "Just as I predicted."
msgstr "Proprio come avevo previsto."

msgctxt "2\t52898\t242\tCHK"
msgid "All right."
msgstr "Va bene."

msgctxt "2\t52909\t243\tCHK"
msgid "I'm not giving up."
msgstr "Non mi arrendo."

msgctxt "2\t52928\t244\tCHK"
msgid "I do <Italic>not</Italic> want to lose."
msgstr "Non voglio <Italic>assolutamente</Italic> perdere."

msgctxt "2\t52968\t245\tCHK"
msgid "Gotta find a job."
msgstr "Devo trovare un lavoro."

msgctxt "2\t52986\t246\tCHK"
