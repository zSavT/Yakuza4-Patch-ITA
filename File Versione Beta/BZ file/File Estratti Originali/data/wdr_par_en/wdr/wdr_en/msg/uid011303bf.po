msgstr ""

msgctxt "2988_Saejima"
msgid "Efficiency is most important."
msgstr ""

msgctxt "3000_Saejima"
msgid "Spirit is most important."
msgstr ""

msgctxt "3012_Saejima"
msgid "Don't know."
msgstr ""

msgctxt "3024_Saejima"
msgid "Go to a new dojo."
msgstr ""

msgctxt "3036_Saejima"
msgid "Wouldn't care."
msgstr ""

msgctxt "3048_Saejima"
msgid "I'd chase 'em off."
msgstr ""

msgctxt "3060_Saejima"
msgid "I don't do that."
msgstr ""

msgctxt "3072_Saejima"
msgid "I do it everyday."
msgstr ""

msgctxt "3084_Saejima"
msgid "No, but I wanna try."
msgstr ""

msgctxt "3096_Saejima"
msgid "Don't worry 'bout that now."
msgstr ""

msgctxt "3108_Saejima"
msgid "Enjoy life as a handsome champion."
msgstr ""

msgctxt "3120_Saejima"
msgid "I'll be yer competition."
msgstr ""