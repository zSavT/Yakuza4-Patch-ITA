msgid "Recent Celebrities"
msgstr ""

msgctxt "3144_Saejima"
msgid "Comedians"
msgstr ""

msgctxt "3156_Saejima"
msgid "Sodachi"
msgstr ""

msgctxt "3168_Saejima"
msgid "Ehama"
msgstr ""

msgctxt "3180_Saejima"
msgid "Myself"
msgstr ""

msgctxt "3192_Saejima"
msgid "His devastatin' strength."
msgstr ""

msgctxt "3204_Saejima"
msgid "His cool mask."
msgstr ""

msgctxt "3216_Saejima"
msgid "His sense o' justice."
msgstr ""

msgctxt "3228_Saejima"
msgid "Win back yer love!"
msgstr ""

msgctxt "3240_Saejima"
msgid "Don't worry."
msgstr ""

msgctxt "3252_Saejima"
msgid "Not at all."
msgstr ""

msgctxt "3264_Saejima"
msgid "Hmph. Yeah, a little."
msgstr ""

msgctxt "3276_Saejima"
msgid "(I should pretend I know.)"
msgstr ""

msgctxt "3288_Saejima"
msgid "Techno...? Never heard of it."
msgstr ""

msgctxt "3300_Saejima"
msgid "Definitely."
msgstr ""

msgctxt "3312_Saejima"
msgid "That ain't likely."
msgstr ""

