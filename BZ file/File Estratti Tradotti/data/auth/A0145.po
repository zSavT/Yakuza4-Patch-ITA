msgctxt "492\t573"
msgid "I'll be back. Tomorrow."
msgstr "Sarò di ritorno. Domani."

msgctxt "607\t674"
msgid "Looking forward to it."
msgstr "Non vedo l'ora."

msgctxt "975\t1031"
msgid "Hey, uh..."
msgstr "Ehi, ehm..."

msgctxt "1056\t1104"
msgid "Your name."
msgstr "Il vostro nome."

msgctxt "1105\t1178"
msgid "I never asked you your name."
msgstr "Non ve l'ho chiesto."

msgctxt "1275\t1335"
msgid "Lily."
msgstr "Lily."

msgctxt "1341\t1392"
msgid "It's Lily."
msgstr "È Lily."

msgctxt "1413\t1473"
msgid "Lily-san?"
msgstr "Lily-san?"

msgctxt "1474\t1581"
msgid "That your real name?\\nOr just the one you use at work?"
msgstr "È il vostro vero nome?\\nO quello del lavoro?"

msgctxt "1659\t1720"
msgid "Does it really matter either way?"
msgstr "Ha qualche importanza?"

msgctxt "1775\t1845"
msgid "No... Guess it doesn't."
msgstr "No... Immagino di no."

msgctxt "1899\t1976"
msgid "See you tomorrow."
msgstr "Ci vediamo domani."

