msgctxt "1041\t1097"
msgid "Majima-san..."
msgstr "Majima-san..."

msgctxt "1430\t1485"
msgid "Majima-san!"
msgstr "Majima-san!"

msgctxt "1549\t1596"
msgid "Kiryu-chan."
msgstr "Kiryu-chan."

msgctxt "1623\t1666"
msgid "Hey, don't move!"
msgstr "Ehi, non muoverti!"

msgctxt "1671\t1700"
msgid "That's far enough!"
msgstr "Basta così!"

msgctxt "1712\t1752"
msgid "What happened here?"
msgstr "Cos'è successo qui?"

msgctxt "1766\t1922"
msgid "They trapped me like a freakin' rat.\\nGuess I'm gettin' cropped out for a while."
msgstr "Mi hanno rinchiuso come un ratto.\\nMi sa che sarò fuori dai giochi per un po'."

msgctxt "1932\t1977"
msgid "Who's behind this?"
msgstr "Chi c'è dietro a tutto?"

msgctxt "2005\t2041"
msgid "Daigo."
msgstr "Daigo."

msgctxt "2052\t2091"
msgid "What!?"
msgstr "Cosa!?"

msgctxt "2107\t2163"
msgid "Daigo Dojima."
msgstr "Daigo Dojima."

msgctxt "2199\t2247"
msgid "But why would he...?"
msgstr "Perché dovrebbe...?"

msgctxt "2258\t2320"
msgid "That's enough chit-chat!"
msgstr "Basta chiacchiere!"

msgctxt "2325\t2375"
msgid "You there, stay back!"
msgstr "Tu lì, stai indietro!"

msgctxt "2380\t2463"
msgid "Unless you want to get hauled in, too!"
msgstr "A meno che tu non voglia fargli compagnia!"

msgctxt "2596\t2659"
msgid "Better hurry, Kiryu-chan."
msgstr "Meglio sbrigarsi, Kiryu-chan."

msgctxt "2719\t2797"
msgid "It's all one big spider web."
msgstr "È tutta una grande ragnatela."

msgctxt "2807\t2928"
msgid "The hit back in '85.\\nThe ten billion yen."
msgstr "Il colpo dell'85.\\nI dieci miliardi di yen."

msgctxt "2929\t2990"
msgid "Even Yasuko-chan."
msgstr "Anche Yasuko-chan."

msgctxt "3007\t3065"
msgid "What!?"
msgstr "Cosa!?"

msgctxt "3079\t3206"
msgid "If we don't do somethin'...\\nthe Tojo's gonna bite the dust."
msgstr "Se non facciamo qualcosa...\\nIl Tojo verrà distrutto."

msgctxt "3207\t3308"
msgid "Yasuko-chan's in danger, too."
msgstr "Anche Yasuko-chan è in pericolo."

msgctxt "3357\t3417"
msgid "This is on you."
msgstr "Dipende tutto da te."

msgctxt "3434\t3510"
msgid "You're all we got."
msgstr "Sei tutto ciò che ci rimane."

