msgstr ""

msgctxt "3640_Saejima"
msgid "Take yer time."
msgstr ""

msgctxt "3652_Saejima"
msgid "Enough! Gimme the damn menu!"
msgstr ""

msgctxt "3664_Saejima"
msgid "The reason ain't important."
msgstr ""

msgctxt "3676_Saejima"
msgid "It ain't impure."
msgstr ""

msgctxt "3688_Saejima"
msgid "You're pathetic!"
msgstr ""